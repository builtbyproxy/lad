    "node": ">=8.9.0"
  },
  "homepage": "<%= repo %>",
  "keywords": [
    "<%= this.slug(name) %>",
    "lad",
    "lass"
  ],
  "license": "Unlicensed",
  "lint-staged": {
    "linters": {
      "*.{js,jsx,mjs,ts,tsx,css,less,scss,json,graphql}": [
        "prettier --write",
        "git add"
      ],
      "*.md": [
        "git add"
      ],
      "*.md,!test/snapshots/**/*.md,!test/**/snapshots/**/*.md,!locales/README.md": [
        "remark . -qfo"
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "trailingComma": "none"
  },
  "private": true,
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/lad"
  },
  "scripts": {
    "build": "nps build",
    "coverage": "nps coverage",
    "lint": "nps lint",
    "precommit": "nps precommit",
    "pretest": "nps pretest",
    "start": "nps",
    "test": "nps test",
    "test-coverage": "nps test-coverage",
    "watch": "nps watch"
  },
  "xo": {
    "extends": "prettier",
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": true,
          "trailingComma": "none"
        }
      ],
      "max-len": [
        "error",
        {
          "code": 100,
          "ignoreUrls": true
        }
      ],
      "capitalized-comments": "off",
      "camelcase": "off",
      "no-warning-comments": "off"
    },
    "space": true,
    "overrides": [
      {
        "plugins": [
          "prettier"
        ],
        "files": [
          "test/*.js",
          "test/**/*.js"
        ],
        "globals": [
          "web",
          "api"
        ]
      },
      {
        "plugins": [
          "prettier",
          "compat"
        ],
        "settings": {
          "polyfills": [
            "fetch"
          ]
        },
        "parserOptions": {
          "sourceType": "script"
        },
        "files": [
          "assets/js/*.js",
          "assets/js/**/*.js"
        ],
        "envs": [
          "browser"
        ],
        "rules": {
          "compat/compat": "error"
        }
      }
    ]
  }
}
